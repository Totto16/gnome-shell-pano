{
  "compilerOptions": {
    "lib": ["ES2021"],
    "target": "ES2021",
    "strict": true,
    "noImplicitAny": false,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "strictPropertyInitialization": false,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./src/",
    "moduleResolution": "Node",
    "experimentalDecorators": true,
    "paths": {
      "@imports/*": ["../@types/*"],
      "@pano/*": ["./*"],
      "@gnome-shell/ui/layout": ["./types/ui/layout/index.d.ts"],
      "@gnome-shell/ui/main": ["./types/ui/main/index.d.ts"],
      "@gnome-shell/misc/util": ["./types/misc/util/index.d.ts"],
      "@gnome-shell/ui/messageTray": ["./types/ui/messageTray/index.d.ts"]
    }
  },
  "exclude": ["./dist"]
}
